{
  "name": "api-server",
  "version": "1.0.0",
  "description": "Backend Boilerplate",
  "main": "src/index.js",
  "exports": "./src/index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json && cp -r ./src/public ./dist/src ",
    "dev": "ts-node -P ./tsconfig-transpile-only.json  -r module-alias/register ./node_modules/moleculer/bin/moleculer-runner.js -i 1 --config ./moleculer.config.ts --hot --mask **/*.service.* src/services src/*api* ",
    "dev:repl": "ts-node -P ./tsconfig-transpile-only.json -r module-alias/register ./node_modules/moleculer/bin/moleculer-runner.js --config ./moleculer.config.ts --repl --hot  --mask **/*.service.* src/services src/*api* ",
    "start": "cd dist/ && moleculer-runner --config ./moleculer.config.js -i 2  ./src/services ./src/*api*",
    "start-dev": "pnpm run dev"
  },
  "keywords": [],
  "author": "GreenRenge",
  "license": "ISC",
  "dependencies": {
    "@moleculer/lab": "^0.6.4",
    "caller-path": "^3.0.1",
    "cookie-parser": "^1.4.7",
    "directory-import": "^2.3.1",
    "extract-files": "^13.0.0",
    "fastest-validator": "^1.19.0",
    "graphql": "^14.7.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.2",
    "lodash": "^4.17.21",
    "moleculer": "0.15.0-beta1",
    "moleculer-apollo-server": "^0.3.8",
    "moleculer-repl": "^0.7.4",
    "moleculer-web": "0.11.0-beta1",
    "nats": "^2.28.2",
    "@supabase/supabase-js": "2.49.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.4.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "esm-module-alias": "^2.2.1",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.1.0",
    "rimraf": "^6.0.1",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0",
    "ts-jest": "^29.2.5",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.7.2"
  }
}